<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/fragment_user_details_layout_margin">


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_user_repos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="false"
        android:paddingStart="@dimen/rv_user_repos_padding_start"
        android:paddingTop="@dimen/rv_user_repos_padding_top"
        android:paddingEnd="@dimen/rv_user_repos_padding_end"
        android:paddingBottom="@dimen/rv_user_repos_padding_bottom">

    </androidx.recyclerview.widget.RecyclerView>
</LinearLayout>